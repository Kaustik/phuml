{
  "name": "kaustik/phuml",
  "type": "System",
  "description": "Composer file for PhUML.",
  "license": "mit?",
  "require-dev": {
    "phpunit/phpunit": "4.8.*",
    "symfony/console": "3.*"
  },
  "autoload": {
    "psr-4": {
      "Phuml\\": "src",
      "Test\\": "tests"
    },
    "classmap": [
      "src/classes",
      "src/interfaces",
      "src/exceptions"
    ]
  },
  "config": {
    "bin-dir": "bin"
  }
}
